    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules",
    "docs": "node_modules/.bin/aglio -i resources/assets/docs/api.md -o public/docs/index.html --theme-full-width -t slate -n resources/assets/docs",
    "watch-docs": "node_modules/.bin/aglio -i resources/assets/docs/api.md -o public/docs/index.html --theme-full-width -t slate -n resources/assets/docs -s -h 0.0.0.0"
